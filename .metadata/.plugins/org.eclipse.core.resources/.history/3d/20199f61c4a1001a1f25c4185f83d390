/*
public class Main {

	public static void main(String[] args) {
		Locale.setDefault(Locale.US);
	
		Scanner sc = new Scanner(System.in);
		
		String x;
		int y;
		double z;	
		
		x = sc.next();
		y = sc.nextInt();
		z = sc.nextDouble();
		
		System.out.println("Dados Inseridos: " + "\n" + x + "\n"  + y + "\n" + z);		
				
		sc.close();		

	}

// 5.

public class Main {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int number1;
		int number2;
		float x;
	System.out.println("What did you order? (Item - Quantity)");
	number1 = sc.nextInt();
	number2 = sc.nextInt();
	if (number1 == 1) {
		x = (float) (4.00 * number2);
	}
	else if (number1 == 2) {
		x = (float) (4.50 * number2);
		}
	else if (number1 == 3) {
		x = (float) (5.00 * number2);
	}
	else if (number1 == 4) {
		x = (float) (2.00 * number2);
	}
	else { 
		x = (float) (1.50 * number2);
	}
	System.out.println("Total: US$ " + x);
	sc.close();
	}}
